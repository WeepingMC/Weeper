From 7c482b420f503c2f689cafa342f0651de744f5c0 Mon Sep 17 00:00:00 2001
From: Thonk <30448663+ExcessiveAmountsOfZombies@users.noreply.github.com>
Date: Sun, 13 Dec 2020 23:12:43 -0600
Subject: [PATCH] add get/set biome for namespaced keys

---
 src/main/java/org/bukkit/World.java       | 22 ++++++++++++++++++++++
 src/main/java/org/bukkit/block/Block.java | 17 +++++++++++++++++
 2 files changed, 39 insertions(+)

diff --git a/src/main/java/org/bukkit/World.java b/src/main/java/org/bukkit/World.java
index 042baabf..0b69aef2 100644
--- a/src/main/java/org/bukkit/World.java
+++ b/src/main/java/org/bukkit/World.java
@@ -2433,6 +2433,28 @@ public interface World extends PluginMessageRecipient, Metadatable {
      */
     @NotNull
     Biome getBiome(int x, int y, int z);
+    // Paper start
+    /**
+     * Gets the biome NamespacedKey for the given block coordinates.
+     *
+     * @param x X coordinate of the block
+     * @param y Y coordinate of the block
+     * @param z Z coordinate of the block
+     * @return Biome NamespacedKey of the requested block
+     */
+    @NotNull
+    NamespacedKey getBiomeKey(int x, int y, int z);
+
+    /**
+     * Sets the biome for the given block coordinates
+     *
+     * @param x X coordinate of the block
+     * @param y Y coordinate of the block
+     * @param z Z coordinate of the block
+     * @param key new Biome from a namespaced key
+     */
+    void setBiome(int x, int y, int z, @NotNull NamespacedKey key);
+    // Paper end
 
     /**
      * Sets the biome for the given block coordinates
diff --git a/src/main/java/org/bukkit/block/Block.java b/src/main/java/org/bukkit/block/Block.java
index 0c72d00a..3bbae1e0 100644
--- a/src/main/java/org/bukkit/block/Block.java
+++ b/src/main/java/org/bukkit/block/Block.java
@@ -6,6 +6,7 @@ import org.bukkit.Chunk;
 import org.bukkit.FluidCollisionMode;
 import org.bukkit.Location;
 import org.bukkit.Material;
+import org.bukkit.NamespacedKey;
 import org.bukkit.World;
 import org.bukkit.block.data.Bisected;
 import org.bukkit.block.data.BlockData;
@@ -354,6 +355,22 @@ public interface Block extends Metadatable {
     @NotNull
     Biome getBiome();
 
+    // paper start
+    /**
+     * Returns the key of the biome that this block resides in
+     *
+     * @return NamespacedKey of a biome from this block.
+     */
+    @NotNull
+    NamespacedKey getBiomeKey();
+
+    /**
+     * Sets the biome that this block resides in
+     *
+     * @param key new namespaced biome type for this block
+     */
+    void setBiome(@NotNull NamespacedKey key);
+    // paper end
     /**
      * Sets the biome that this block resides in
      *
